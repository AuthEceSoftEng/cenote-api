{
  "name": "cenote-api-server",
  "version": "0.1.0",
  "private": true,
  "description": "API server & web client used by cenote",
  "homepage": "https://github.com/AuthEceSoftEng/cenote-api#readme",
  "bugs": {
    "url": "https://github.com/AuthEceSoftEng/cenote-api/issues"
  },
  "repository": "github:AuthEceSoftEng/cenote-api",
  "license": "MIT",
  "author": {
    "name": "Napoleon Oikonomou",
    "email": "Napoleonoikon@gmail.com",
    "url": "https://iamnapo.me"
  },
  "scripts": {
    "build": "cd client && npm run build",
    "clean": "concurrently \"rm -rf node_modules && npm i\" \"npm run clean --prefix=\"./client\"\"",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "npm run open:src --prefix=\"./client\"",
    "dev:server": "nodemon server",
    "postinstall": "npm i --prefix=\"./client\"",
    "lint:client": "npm run lint --prefix=\"./client\"",
    "lint:server": "eslint server/**/* --fix",
    "start": "node server",
    "test:server": "jest server"
  },
  "jest": {
    "collectCoverageFrom": [
      "server/**/*.{js}"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "testURL": "http://localhost"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.2",
    "connect-mongodb-session": "^2.0.6",
    "dotenv": "^6.2.0",
    "drpcjs": "^0.1.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "mongoose": "^5.4.5",
    "mongoose-auto-increment-reworked": "^1.2.1",
    "mongoose-immutable": "^0.0.2",
    "nodemailer": "^5.1.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "ramda": "^0.26.1",
    "sinek": "^6.24.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "concurrently": "^4.1.0",
    "eslint": "^5.12.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-iamnapo": "^1.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.4",
    "jest": "^23.6.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.9",
    "supertest": "^3.4.1"
  },
  "engines": {
    "node": ">=10"
  }
}
