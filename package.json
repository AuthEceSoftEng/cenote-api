{
  "name": "smack-bdms-web",
  "version": "0.1.0",
  "private": true,
  "description": "BDMS Management",
  "homepage": "https://github.com/AuthEceSoftEng/smack-bdms-web#readme",
  "bugs": {
    "url": "https://github.com/AuthEceSoftEng/smack-bdms-web/issues"
  },
  "repository": "github:AuthEceSoftEng/smack-bdms-web",
  "license": "MIT",
  "author": {
    "name": "Napoleon Oikonomou",
    "email": "Napoleonoikon@gmail.com",
    "url": "https://iamnapo.me"
  },
  "scripts": {
    "build": "cd client && yarn build",
    "clean": "concurrently \"rm -rf node_modules && rm yarn.lock & yarn\" \"cd client && yarn clean\"",
    "dev": "concurrently \"yarn dev:server\" \"yarn dev:client\"",
    "dev:client": "cd client && yarn open:src",
    "dev:server": "nodemon server",
    "lint:client": "cd client && yarn lint",
    "lint:server": "eslint .",
    "test:server": "jest server",
    "start": "node server"
  },
  "jest": {
    "collectCoverageFrom": [
      "server/**/*.{js}"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "testURL": "http://localhost"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "connect-mongodb-session": "^2.0.3",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "mongoose": "^5.3.4",
    "mongoose-auto-increment-reworked": "^1.2.1",
    "mongoose-immutable": "^0.0.2",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "ramda": "^0.25.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "concurrently": "^4.0.1",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.6.0",
    "nodemon": "^1.18.4",
    "supertest": "^3.3.0"
  },
  "engines": {
    "node": ">=8"
  }
}
